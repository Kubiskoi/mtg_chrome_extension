{
  "manifest_version": 2,

  "name": "Tournament manager",
  "description": "This extension will manage tournament adding on mtg.sk",
  "version": "1.0",

  //set up instalation icon
  "icons":{
    "128":"icon128.png"
  },
  
  "browser_action": {
    //set up icon next to  chrome Omnibox
   "default_icon":"icon19.png",
   //html popped on icon click
   "default_popup": "popup.html"
  },
  
  "permissions": [
  "activeTab"
  ]
}